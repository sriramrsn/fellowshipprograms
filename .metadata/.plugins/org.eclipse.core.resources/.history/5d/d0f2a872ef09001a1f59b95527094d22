package com.bridgelabz.fellowshipprograms.functional;
import java.util.Scanner;

public class Array {
	public static void array(int i,int j)
	{
		int [][] a= new int[i][j];
		Scanner sc=new Scanner(System.in);
		System.out.println("enter a array elements:");
		for(int r=0;r<i;r++)
		{
			for(int c=0;c<j;c++)
			{
				a[r][c]=sc.nextInt();
			}
		}
		for(int r=0;r<i;r++)
		{
			for(int c=0;c<j;c++)
			{
				System.out.print("\t" +a[r][c]);			
				}
			System.out.println();
		}
		
		
	}
	
	public static void main(String args[])
	{
	Scanner sc = new Scanner(System.in);
	System.out.print("enter a row and column");
	int i= sc.nextInt();
	int j=sc.nextInt();
	array(i, j);
	sc.close();
}
}
