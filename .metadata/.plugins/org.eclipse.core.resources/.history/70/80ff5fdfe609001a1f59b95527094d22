package com.bridgelabz.fellowshipprograms.basiccore;
import com.bridgelabz.fellowshipprograms.utility.*;
public class Factors {
	public static void main(String args[])
	{
		int []a=new int[100];
		System.out.println("Enter prime factor");
		int n=Utility.sc.nextInt();
		a=Utility.primeFactor(n);
		for(int i=0;i<n;i++)
		{
		System.out.print("prime factor "+a[i]);
	}
	}
}
